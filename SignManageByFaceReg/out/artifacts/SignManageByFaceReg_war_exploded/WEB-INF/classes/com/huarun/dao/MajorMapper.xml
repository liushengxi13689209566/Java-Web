        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huarun.dao.MajorMapper">
    <select id="getMajorInfoByID" resultType="MajorInfo">
        select * from SignManage.major where
        major_id =  #{major_id}
    </select>

